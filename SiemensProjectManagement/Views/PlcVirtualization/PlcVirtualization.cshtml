                        var elementred = document.getElementById(name + "_red" + count);
                        if (elementgreen !== null || elementyellow !== null || elementred !== null) {
                            if (plc.Operating_State === 'Run') {

                                //elementgreen.style.animation = "none";
                                ////element.style.webkitAnimation = "none";
                                elementgreen.style.webkitAnimation = "blinkGreen 1s infinite";
                                elementyellow.style.webkitAnimation = "none";
                                elementred.style.webkitAnimation = "none";

                            }
                            else if (plc.Operating_State === 'Stop') {
                                elementyellow.style.webkitAnimation = "blinkYellow 1s infinite";
                                elementgreen.style.webkitAnimation = "none";
                                elementred.style.webkitAnimation = "none";
                            }

                            else if (plc.Operating_State === 'Fault') {
                                elementred.style.webkitAnimation = "blinkRed 1s infinite";
                                elementgreen.style.webkitAnimation = "none";
                                elementyellow.style.webkitAnimation = "none";
                            }
                            else {
                                elementred.style.webkitAnimation = "none";
