<script type="text/javascript">
    var _sideHide = false;

    var plcData = []; var button_Id = "";
    $(document).ready(function () {

        GetData();



        //var element = document.getElementById("Rack1_Plc1_red1");
        //element.style.webkitAnimation = "blinkRed 1s infinite";


        //var element = document.getElementById("Rack1_Plc1_green1");
        //element.style.animation = "";
        //element.style.webkitAnimation = "";


    });

     function GetData() {
         $.getJSON("@Url.Action("GetPlcdata", "PlcVirtualization")", { plcid: '1' }, function (data) {
                if (data) {
                    plcData = data;

                    //////////////Start And stop or Fault plc check logic goes here////////////////
                    debugger;
                    var count = 1;
                    _.each(plcData, function (plc) {
                        var rackNo = plc.Rack_No.split('_');
                        var name = rackNo[0] + '_' + "Plc" + plc.PlcId;

                        var elementgreen = document.getElementById(name + "_green" + count);
                        var elementyellow = document.getElementById(name + "_yellow" + count);
                        var elementred = document.getElementById(name + "_red" + count);
                        if (plc.Operating_State === 'Run') {

                            //elementgreen.style.animation = "none";
                            ////element.style.webkitAnimation = "none";
                            elementgreen.style.webkitAnimation = "blinkGreen 1s infinite";
                            elementyellow.style.webkitAnimation = "none";
                            elementred.style.webkitAnimation = "none";

                        }
                        else if (plc.Operating_State === 'Stop') {
                            elementyellow.style.webkitAnimation = "blinkYellow 1s infinite";
                            elementgreen.style.webkitAnimation = "none";
                            elementred.style.webkitAnimation = "none";
                        }

                        else if (plc.Operating_State === 'Fault') {
                            elementred.style.webkitAnimation = "blinkRed 1s infinite";
                            elementgreen.style.webkitAnimation = "none";
                            elementyellow.style.webkitAnimation = "none";
                        }
                        else {
                            elementred.style.webkitAnimation = "none";
                            elementgreen.style.webkitAnimation = "none";
                            elementyellow.style.webkitAnimation = "none";
                        }
                        count++;
                    });

                }
         });



        }
    $(".btn ").click(function () {

        debugger;

        button_Id = this.id;
        var array = button_Id.split('_');
        var rackNo = array[0].replace("Rack", "");
        var plcNo = array[1].replace("Plc", "");


        var _plcinfo = _.filter(plcData, function (item) {

            return item.PlcId === parseInt(plcNo);
        });

        document.getElementById('txtIpaddress').value = _plcinfo[0].IpAddress;
        document.getElementById('txtMac_Address').value = _plcinfo[0].Mac_Address;
        document.getElementById('txtState').value = _plcinfo[0].Operating_State;
        document.getElementById('txtType').value = _plcinfo[0].PLC_Type;
        document.getElementById('txtName').value = _plcinfo[0].PLC_Name;
        document.getElementById('txtSerialNo').value = _plcinfo[0].Plc_SerialNo;
        document.getElementById('txtFirmware').value = _plcinfo[0].Firmware;
        document.getElementById('txtMlfb').value = _plcinfo[0].Mlfb;
        document.getElementById('txtSMCSerialNo').value = _plcinfo[0].SMC_SerialNo;
    });
