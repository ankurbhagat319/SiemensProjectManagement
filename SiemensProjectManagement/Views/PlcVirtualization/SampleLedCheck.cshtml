            #card figure {
                margin: 0;
                display: block;
                position: absolute;
                width: 100%;
                height: 100%;
                backface-visibility: hidden;
            }

            #card .front {
                background: red;
            }

            #card .back {
                background: blue;
                transform: rotateY( 180deg);
            }

            #card.flipped {
                transform: rotateY( 180deg);
            }

        .led-red {
            margin: 0 auto;
            width: 6px;
            height: 6px;
            background-color: #3d0404;
            border-radius: 50%;
            box-shadow: rgba(0, 0, 0, 0.2) 0 -1px 7px 1px, inset #441313 0 -1px 9px, rgba(255, 0, 0, 0.5) 0 2px 12px;
