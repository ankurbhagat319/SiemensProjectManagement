        card.classList.toggle('flipped');

    }, false);
    function fnFlip() {

        if (_sideHide) {

            _sideHide = false;
            document.getElementsByClassName('front')[0].style.display = 'inline';
        }
        else {
            _sideHide = true;


            document.getElementsByClassName('front')[0].style.display = 'none';
        }

    }


    function fnLedOnOff() {
        var element = document.getElementById("Rack1_Plc1_red1");
